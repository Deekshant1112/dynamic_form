<!doctype html><html><head><meta charset='utf-8'><title>Submissions</title><link rel='stylesheet' href='/css/theme.css'></head><body class='bg-gradient'><div class='dashboard-container glass'><h1>Submissions</h1><p><a href='/admin' class='btn'>Back</a></p><table><thead><tr><th>Form</th><th>Answers</th><th>Submitted At</th><th>Actions</th></tr></thead><tbody><% submissions.forEach(function(s){ %><tr><td><%= s.formId %></td><td><pre><%= JSON.stringify(s.answers, null, 2) %></pre></td><td><%= s.submittedAt.toISOString() %></td><td><form method='post' action='/admin/forms/<%= s.formId %>/submissions/<%= s._id %>/delete' onsubmit='return confirm("Delete submission?")'><button class='btn small danger'>Delete</button></form></td></tr><% }) %></tbody></table></div></body></html>