<!doctype html><html><head><meta charset='utf-8'><title>Admin - Dashboard</title><link rel='stylesheet' href='/css/theme.css'></head><body class='bg-gradient'><div class='dashboard-container glass'><header><h1>Admin Dashboard</h1><nav><a href='/admin/forms/create' class='btn primary'>+ Create New Form</a><a href='/admin/login' class='btn'>Logout</a></nav></header><section class='stats'><div class='stat-card'><h3>Total forms</h3><p class='big'><%= totalForms %></p></div><div class='stat-card'><h3>Total submissions</h3><p class='big'><%= submissionsCount %></p></div></section><section class='table-section'><h2>Existing Forms</h2><% if (forms.length === 0) { %><p class='no-data'>No forms available yet. Create one to get started.</p><% } else { %><table><thead><tr><th>Title</th><th>Description</th><th>Actions</th></tr></thead><tbody><% forms.forEach(function(f){ %><tr><td><%= f.title %></td><td><%= f.description %></td><td class='actions'><a href='/forms/<%= f._id %>' class='btn small'>View</a><a href='/admin/forms/<%= f._id %>/edit' class='btn small edit'>Edit</a><form method='post' action='/admin/forms/<%= f._id %>/delete' onsubmit='return confirm("Delete form?")' class='inline-form'><button type='submit' class='btn small danger'>Delete</button></form><a href='/admin/forms/<%= f._id %>/submissions' class='btn small info'>Submissions</a><a href='/admin/forms/<%= f._id %>/fields' class='btn small'>Fields</a></td></tr><% }) %></tbody></table><% } %></section></div></body></html>